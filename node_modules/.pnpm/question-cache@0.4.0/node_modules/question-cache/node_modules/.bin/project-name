#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/musicbackend/node_modules/.pnpm/project-name@0.2.6/node_modules/project-name/node_modules:/home/theia/musicbackend/node_modules/.pnpm/project-name@0.2.6/node_modules:/home/theia/musicbackend/node_modules/.pnpm/node_modules:/home/theia/musicbackend/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/musicbackend/node_modules/.pnpm/question-cache@0.4.0/node_modules/question-cache/node_modules:/home/theia/musicbackend/node_modules/.pnpm/question-cache@0.4.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/musicbackend/node_modules/.pnpm/project-name@0.2.6/node_modules/project-name/node_modules:/home/theia/musicbackend/node_modules/.pnpm/project-name@0.2.6/node_modules:/home/theia/musicbackend/node_modules/.pnpm/node_modules:/home/theia/musicbackend/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/musicbackend/node_modules/.pnpm/question-cache@0.4.0/node_modules/question-cache/node_modules:/home/theia/musicbackend/node_modules/.pnpm/question-cache@0.4.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../project-name@0.2.6/node_modules/project-name/cli.js" "$@"
else
  exec node  "$basedir/../../../../../project-name@0.2.6/node_modules/project-name/cli.js" "$@"
fi
