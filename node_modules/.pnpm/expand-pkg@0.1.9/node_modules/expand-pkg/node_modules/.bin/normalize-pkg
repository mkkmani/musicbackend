#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/musicbackend/node_modules/.pnpm/normalize-pkg@0.3.20/node_modules/normalize-pkg/node_modules:/home/theia/musicbackend/node_modules/.pnpm/normalize-pkg@0.3.20/node_modules:/home/theia/musicbackend/node_modules/.pnpm/node_modules:/home/theia/musicbackend/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/musicbackend/node_modules/.pnpm/expand-pkg@0.1.9/node_modules/expand-pkg/node_modules:/home/theia/musicbackend/node_modules/.pnpm/expand-pkg@0.1.9/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/musicbackend/node_modules/.pnpm/normalize-pkg@0.3.20/node_modules/normalize-pkg/node_modules:/home/theia/musicbackend/node_modules/.pnpm/normalize-pkg@0.3.20/node_modules:/home/theia/musicbackend/node_modules/.pnpm/node_modules:/home/theia/musicbackend/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/musicbackend/node_modules/.pnpm/expand-pkg@0.1.9/node_modules/expand-pkg/node_modules:/home/theia/musicbackend/node_modules/.pnpm/expand-pkg@0.1.9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../normalize-pkg@0.3.20/node_modules/normalize-pkg/cli.js" "$@"
else
  exec node  "$basedir/../../../../../normalize-pkg@0.3.20/node_modules/normalize-pkg/cli.js" "$@"
fi
