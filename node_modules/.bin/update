#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/musicbackend/node_modules/.pnpm/update@0.7.4/node_modules/update/bin/node_modules:/home/theia/musicbackend/node_modules/.pnpm/update@0.7.4/node_modules/update/node_modules:/home/theia/musicbackend/node_modules/.pnpm/update@0.7.4/node_modules:/home/theia/musicbackend/node_modules/.pnpm/node_modules:/home/theia/musicbackend/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/musicbackend/node_modules/update/bin/node_modules:/home/theia/musicbackend/node_modules/update/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/musicbackend/node_modules/.pnpm/update@0.7.4/node_modules/update/bin/node_modules:/home/theia/musicbackend/node_modules/.pnpm/update@0.7.4/node_modules/update/node_modules:/home/theia/musicbackend/node_modules/.pnpm/update@0.7.4/node_modules:/home/theia/musicbackend/node_modules/.pnpm/node_modules:/home/theia/musicbackend/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/musicbackend/node_modules/update/bin/node_modules:/home/theia/musicbackend/node_modules/update/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update/bin/update.js" "$@"
else
  exec node  "$basedir/../update/bin/update.js" "$@"
fi
